            <div ng-app="Prueba">
                <div ng-controller="Trabajo1">
                        Registrar nuevo usuario:

			<div class="card">

                            <div class="card-block">
                        <div class="modal fade" id="modal-container-221606" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
                            <div class="modal-dialog" role="document">
                                <div class="modal-content">
                                    <div class="modal-header">
                                        <h5 class="modal-title" id="myModalLabel">
                                            Editar Usuarios
                                        </h5> 
                                        <button id="cierramodal" type="button" class="close" data-dismiss="modal">
                                            <span aria-hidden="true">×</span>
                                        </button>
                                    </div>
                                    <div class="modal-body">
                                        <div class="form-group">
                                            <label class="col-md-12">Nombre: </label>
                                            <div class="col-md-12">
                                                <input type="text" placeholder="" ng-model="nombreEdit" class="form-control form-control-line">
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label for="example-email" class="col-md-12">Apellidos:</label>
                                            <div class="col-md-12">
                                                <input type="text" placeholder="" class="form-control form-control-line" ng-model="apellidosEdit">
                                            </div>
                                        </div>
                                        
                                        <div class="form-group">
                                            <label class="col-md-12">Tipo de usuario:</label>
                                            <div class="col-md-12">
                                                <select ng-model="tipoEdit" class="form-control form-control-line">
                                                    <option value="-1">Seleccione tipo...</option>
                                                    <option value="Admin">Admin</option>
                                                    <option value="Obras">Obras</option>
                                                    <option value="Caja Chica">Caja chica</option>
                                                    <option value="Diesel">Diesel</option>
                                                    <option value="Mantenimiento">Mantenimiento</option>
                                                </select>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-12">Correo:</label>
                                            <div class="col-md-12">
                                                <input type="text" placeholder="" ng-model="correoEdit" class="form-control form-control-line">
                                            </div>
                                        </div>
                                        <div class="form-group">
                                                <label class="col-md-12">Nombre de usuario:</label>
                                                <div class="col-sm-12">
                                                    <input type="text" placeholder="" ng-model="usuarioEdit" class="form-control form-control-line">
            
                                            </div>
                                        </div>
                                       
                                    </div>
                                    <div class="modal-footer">
                                         <div class="alert alert-danger"  ng-if="errorEdicion">
                                             {{errorEdicion}}
                                        </div>
                                        <button type="button" ng-click="confirma_edicion()" class="btn btn-primary">
                                            Editar
                                        </button> 
                                        <button type="button" class="btn btn-secondary" data-dismiss="modal">
                                            Cancelar
                                        </button>
                                    </div>
                                </div>
                                
                            </div>
                            
                        </div>


                        <!--OTra modal eliminar-->

                        <div class="modal fade" id="modal-container-221607" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
                                <div class="modal-dialog" role="document">
                                    <div class="modal-content">
                                        <div class="modal-header">
                                            <h5 class="modal-title" id="myModalLabel">
                                                Confirmación 
                                            </h5> 
                                            <button id="cierramodal" type="button" class="close" data-dismiss="modal">
                                                <span aria-hidden="true">×</span>
                                            </button>
                                        </div>
                                        <div class="modal-body">
                                             <p>¿Estás seguro de eliminar?</p>
                                        </div>
                                        <div class="modal-footer">
                                             <div class="alert alert-danger"  ng-if="errorBorrar">
                                                 {{errorBorrar}}
                                            </div>
                                            <button type="button" ng-click="confirma_delete()" class="btn btn-primary">
                                                Continuar
                                            </button> 
                                            <button type="button" class="btn btn-secondary" data-dismiss="modal">
                                                Cancelar
                                            </button>
                                        </div>
                                    </div>
                                    
                                </div>
                                
                            </div>
                            <form class="form-horizontal form-material">
                            <div class="form-group">
                                <label class="col-md-12">Nombre: </label>
                                <div class="col-md-12">
                                    <input type="text" placeholder="" ng-model="nombre" class="form-control form-control-line">
                                </div>
                            </div>
                            <div class="form-group">
                                <label for="example-email" class="col-md-12">Apellidos:</label>
                                <div class="col-md-12">
                                    <input type="text" placeholder="" class="form-control form-control-line" ng-model="apellidos">
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-md-12">Password</label>
                                <div class="col-md-12">
                                    <input type="password" ng-model="password" class="form-control form-control-line">
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-md-12">Tipo de usuario:</label>
                                <div class="col-md-12">
                                    <select ng-model="tipo" class="form-control form-control-line">
                                        <option value="-1">Seleccione tipo...</option>
                                        <option value="Admin">Admin</option>
                                        <option value="Obras">Obras</option>
                                        <option value="Caja Chica">Caja chica</option>
                                        <option value="Diesel">Diesel</option>
                                        <option value="Mantenimiento">Mantenimiento</option>
                                    </select>
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-md-12">Correo:</label>
                                <div class="col-md-12">
                                    <input type="email" placeholder="" ng-model="correo" class="form-control form-control-line" autocomplete="off">
                                </div>
                            </div>
                            <div class="form-group">
                                    <label class="col-md-12">Nombre de usuario:</label>
                                    <div class="col-sm-12">
                                        <input type="text" placeholder="" ng-model="usuario" class="form-control form-control-line">

                                </div>
                            </div>
                            <div class="form-group">
                                <div class="col-sm-12">
                                    <button class="btn btn-success" ng-click="Regusuario()">Registrar</button>
                                </div>
                            </div>
                            </form>
                 <div class="alert alert-danger" ng-if="ErrorRegistrousuario">
                     <b>Registre todos los datos </b> 
                   </div>
                   <div class="alert alert-success" ng-if="RegistroExitoso">
                       <b>{{RegistroExitoso.message}}</b>
                      
                     </div> 
                    </div> 
                </div> 
                     <hr>
                     Listado de usuarios

                     <div class="card">
                            <div class="card-block">

                     <div class="table-responsive">
                        <table class="table">
                            <thead>
                              <tr >
                                <th>#</th>
                                <th>Nombre</th>
                                <th>Apellidos</th>
                                <th>Email</th>
                                <th>Usuario</th>
                
                                <th>Contraseña</th>
                                <th>Editar</th>
                                <th>Eliminar</th>



                              </tr>
                            </thead>
                            <tbody>
                              <tr ng-repeat="usuarios in usuarios_consulta">
                                <td> {{$index+1}}</td>
                                <td>{{usuarios.nombre}}</td>
                                <td>{{usuarios.apellidos}}</td>
                                <td>{{usuarios.correo}}</td>
                                <td>{{usuarios.usuario}}</td>

                               
                                <td><button class="btn btn-success">Cambiar contraseña</button></td>
                                <td><button class="btn btn-warning mdi mdi-settings" href="#modal-container-221606" ng-click="mostrarEdicion([usuarios._id])"  data-toggle="modal"> Editar</button></td>
                                <td><button class="btn btn-danger mdi mdi-close" href="#modal-container-221607" ng-click="mostrarEliminar([usuarios._id])"  data-toggle="modal"> Eliminar</button></td>


                              </tr>
                            </tbody>
                          </table>
                
                        </div>
                    
                    </div>
                </div>
   
               </div>
